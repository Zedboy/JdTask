<UserControl x:Class="Jlion.BrushClient.UControl.IpAddressControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Jlion.BrushClient.UControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<UniformGrid Columns="4" TextBoxBase.GotFocus="TextBox_OnGotFocus">
		<DockPanel Margin="5,2">
			<TextBlock VerticalAlignment="Center" DockPanel.Dock="Right"  FontSize="20" Text="." />
			<TextBox Height="30" Width="60" x:Name="IpPartItem1"></TextBox>
		</DockPanel>
		<DockPanel Margin="0,2,5,2">
			<TextBlock VerticalAlignment="Center" DockPanel.Dock="Right"  FontSize="20" Text="." />
			<TextBox Height="30" Width="60" x:Name="IpPartItem2"></TextBox>
		</DockPanel>
		<DockPanel Margin="0,2,5,2">
			<TextBlock VerticalAlignment="Center" DockPanel.Dock="Right" FontSize="20" Text="." />
			<TextBox Height="30" Width="60" x:Name="IpPartItem3"></TextBox>
		</DockPanel>
		<TextBox Height="30" Width="60" x:Name="IpPartItem4"></TextBox>
	</UniformGrid>
</UserControl>
